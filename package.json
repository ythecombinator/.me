{
	"name": "ythecombinator.me",
	"version": "1.0.0",
	"scripts": {
		"build": "next build src",
		"dev": "node server",
		"start": "NODE_ENV=production node server",
		"docs:TOC": "markdown-toc README.md -i",
		"docs:TOC:watch": "onchange 'README.md' -- npm run docs:TOC",
		"test:pre:clean": "rm -rf reports",
		"test:pre:scaffold": "mkdir reports",
		"test:unit": "jest",
		"test:lint": "xo",
		"test:complexity": "plato -r -d reports/complexity -t 'Code Complexity' -x .json src/**/*.js && opn reports/complexity/index.html",
		"test:pwa": "npm run test:pre:clean && npm run test:pre:scaffold && mkdir reports/pwa && lighthouse http://localhost:3000 --output-path  reports/pwa/index --view",
		"test:perf": "sitespeed.io http://localhost:3000 --outputFolder reports/perf && opn reports/perf/index.html",
		"test:regression:reference": "backstop reference",
		"test:regression:compare": "backstop test",
		"test:regression": "npm run test:regression:reference && npm run test:regression:compare",
		"test:dependencies": "npm-check -u"
	},
	"license": "MIT",
	"repository": "ythecombinator/.me",
	"xo": {
		"extends": [
			"prettier",
			"prettier/react",
			"plugin:react/recommended"
		],
		"env": [
			"browser",
			"node"
		],
		"rules": {
			"react/react-in-jsx-scope": 0,
			"import/named": 0,
			"import/order": 0,
			"import/no-extraneous-dependencies": 0,
			"react/prop-types": 0,
			"react/display-name": 0,
			"semi": 1,
			"prefer-default-export": 0
		}
	},
	"jest": {
		"collectCoverage": true,
		"coverageDirectory": "reports/coverage"
	},
	"now": {
		"alias": "ythecombinator.now.sh"
	},
	"dependencies": {
		"babel-runtime": "6.26.0",
		"babili-webpack-plugin": "0.1.2",
		"lighthouse": "2.9.1",
		"module-alias": "2.0.6",
		"next": "5.0.0",
		"next-routes": "1.3.0",
		"postcss": "6.0.19",
		"preact": "8.2.7",
		"preact-compat": "3.18.0",
		"ramda": "0.25.0",
		"react": "16.2.0",
		"react-icon-base": "2.1.2",
		"stackframe": "1.0.4",
		"styled-jsx": "2.2.6",
		"styled-jsx-postcss": "0.2.0",
		"sw-precache-webpack-plugin": "0.11.5",
		"unique-random-array": "1.0.0"
	},
	"devDependencies": {
		"backstopjs": "3.1.19",
		"emoji-tree": "1.1.1",
		"enzyme": "3.3.0",
		"eslint-config-prettier": "2.9.0",
		"eslint-plugin-react": "7.7.0",
		"jest": "22.4.2",
		"markdown-toc": "1.2.0",
		"npm-check": "5.5.2",
		"onchange": "3.3.0",
		"opn-cli": "3.1.0",
		"plato": "1.7.0",
		"prettier": "1.11.1",
		"react-dom": "16.2.0",
		"react-test-renderer": "16.2.0",
		"sitespeed.io": "6.4.1",
		"xo": "0.20.3"
	}
}
