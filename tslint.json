{
  "defaultSeverity": "error",
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "prettier": true,
    "interface-name": false,
    "semicolon": [
      true,
      "always",
      "ignore-interfaces",
      "ignore-bound-class-methods"
    ],
    "member-access": [false],
    "ordered-imports": [true],
    "no-console": [false],
    "no-var-requires": true,
    "quotemark": [true, "single", "jsx-double"]
  },
  "rulesDirectory": []
}
